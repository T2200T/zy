#include <stdio.h>
#include<string.h>
#include<stdlib.h>
int main()
{
	char a[]="",b[]="";
	int m=0,n=0;
	char *ch;
	printf("输入：\n");
	gets(a);
	m=strlen(a);
	printf("输入要查的单词：\n");
	gets(b);
	ch=b;
	for(int i=0;i<m;i++)
	{
		if(a[i]==*ch)
		{
			n++;
		}
	}
	printf("输出：\n");
	printf("%d\n",n);

	
    

	return 0;
}
